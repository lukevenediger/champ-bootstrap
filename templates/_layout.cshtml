<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="description" content="champ-bootstrap">
  <meta name="author" content="">

  <link rel="shortcut icon" type="image/png" href="@ResolveUrl("static/favicon.png")" />
  <link rel="icon" type="image/png" href="@ResolveUrl("static/favicon.png")" />

  <title>champ bootstrap | @Model.Page.Title</title>
  <link href="@ResolveUrl("static/css/bootstrap.min.css")" rel="stylesheet" />
  <link href="@ResolveUrl("static/css/font-awesome.min.css")" rel="stylesheet" />
  <script src="@ResolveUrl("static/scripts/jquery-2.0.3.min.js")"></script>
  <script src="@ResolveUrl("static/scripts/bootstrap.min.js")"></script>
</head>
<body>
  <div id="wrap">

    <!-- Nav Bar -->
    @Include("navbar")

    <!-- Main Content -->
    <div class="container">
      <div class="page-header" id="banner">
        <div class="row">
          <div class="col-lg-8">
            <h2>@Model.Page.Title</h2>
          </div>
        </div>
      </div>

      <div class="row">
        <div class="col-lg-8">
          @RenderBody()
        </div>
      </div>
    </div>
  </div>

  <!-- Footer -->
  @Include("footer")
</body>
</html>
